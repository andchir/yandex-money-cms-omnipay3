{
    "name": "bagart/omnipay3-yandexkassa",
    "type": "library",
    "description": "Payment gateway module for Omnipay: Yandex kassa(checkout) forked from official for version compatible with Laravel 5.6",
    "authors": [
        {
            "name": "Yandex.Money",
            "email": "cms@yamoney.ru"
        },
        {
            "name": "BAGArt",
            "email": "baltaev.artur+opensource@gmail.com"
        }
    ],
    "minimum-stability": "dev",

    "require": {
        "php": ">=5.3.0",
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "yandexmoney\\YandexMoney\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\YandexMoney\\": "tests/"
        }
    }
}
